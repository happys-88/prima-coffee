<div class="modal-dialog modal-dialog-liftgate" role="document">
    <div class="modal-content">
        <div class="modal-header">
            <h4 class="modal-title">No Liftgate Item In Cart</h4> 
            <button type="button" data-mz-action="closePopup" class="close">&times;</button>
        </div>
        <div class="modal-body">
            <div> {{ labels.liftGateRemoved }} </div>
            <div>
                <button type="button" data-mz-action="closePopup" class="mz-button">{{ labels.continue }}</button>
            </div>
        </div>
    </div>
</div>