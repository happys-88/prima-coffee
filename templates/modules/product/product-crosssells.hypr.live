{% if model.items %}
    <div class="mz-product-slider"> 
        <h2 class="heading-2"><span>You May Also Like</span></h2>   
        <ul id="crossSellSlider">              
            {% for productData in model.items %}
                {% include "modules/product/product-list-slider" with model=productData %}        
            {% endfor %} 
        </ul> 
    </div> 
{% endif %} 




